    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "buffer": "^6.0.3",
    "expect": "^27.1.0",
    "jest": "^29.7.0",
    "jest-mock": "^27.1.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
